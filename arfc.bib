	keywords = {atws, Inlet temperature drop, lofc, Molten salt reactor, MSR, Neutron kinetics, Pump coastdown, Rods drop, unread},
	pages = {624--636},
	file = {Analysis_on_the_neutron_kinetics_for_a_molten_salt_reactor.mobi:C\:\\Users\\shkod_000\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\zuhgx6dy.default\\zotero\\storage\\87G849PN\\Analysis_on_the_neutron_kinetics_for_a_molten_salt_reactor.mobi:application/octet-stream;ScienceDirect Full Text PDF:C\:\\Users\\shkod_000\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\zuhgx6dy.default\\zotero\\storage\\3QJ65MRD\\Zhang et al. - 2009 - Analysis on the neutron kinetics for a molten salt.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\shkod_000\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\zuhgx6dy.default\\zotero\\storage\\EAWNFIFZ\\S0149197008001625.html:text/html;zhang_analysis_2008.pdf:C\:\\Users\\shkod_000\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\zuhgx6dy.default\\zotero\\storage\\TG2GD7W2\\zhang_analysis_2008.pdf:application/pdf}
